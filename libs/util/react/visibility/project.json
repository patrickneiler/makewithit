{
  "name": "util-react-visibility",
  "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/util/react/visibility/src",
  "projectType": "library",
  "tags": ["util"],
  "targets": {
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/util/react/visibility/**/*.{ts,tsx,js,jsx}"]
      }
    },
    "build": {
      "executor": "@nx/rollup:rollup",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/util/react/visibility",
        "tsConfig": "libs/util/react/visibility/tsconfig.lib.json",
        "project": "libs/util/react/visibility/package.json",
        "entryFile": "libs/util/react/visibility/src/index.ts",
        "external": ["react", "react-dom", "react/jsx-runtime"],
        "rollupConfig": "@nx/react/plugins/bundle-rollup",
        "compiler": "babel",
        "assets": [
          {
            "glob": "libs/util/react/visibility/README.md",
            "input": ".",
            "output": "."
          }
        ]
      }
    }
  }
}
