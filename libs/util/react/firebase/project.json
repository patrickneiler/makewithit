{
  "name": "util-react-firebase",
  "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/util/react/firebase/src",
  "projectType": "library",
  "tags": [
    "util"
  ],
  "targets": {
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": [
        "{options.outputFile}"
      ],
      "options": {
        "lintFilePatterns": [
          "libs/util/react/firebase/**/*.{ts,tsx,js,jsx}"
        ]
      }
    },
    "build": {
      "executor": "@nx/rollup:rollup",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "outputPath": "dist/libs/util/react/firebase",
        "tsConfig": "libs/util/react/firebase/tsconfig.lib.json",
        "project": "libs/util/react/firebase/package.json",
        "entryFile": "libs/util/react/firebase/src/index.ts",
        "external": [
          "react",
          "react-dom",
          "react/jsx-runtime"
        ],
        "rollupConfig": "@nx/react/plugins/bundle-rollup",
        "compiler": "babel",
        "assets": [
          {
            "glob": "libs/util/react/firebase/README.md",
            "input": ".",
            "output": "."
          }
        ]
      }
    }
  }
}