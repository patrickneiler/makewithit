{
  "name": "feature-react-game",
  "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/feature/react/game/src",
  "projectType": "library",
  "tags": ["feature"],
  "targets": {
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/feature/react/game/**/*.{ts,tsx,js,jsx}"]
      }
    },
    "build": {
      "executor": "@nx/rollup:rollup",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/feature/react/game",
        "tsConfig": "libs/feature/react/game/tsconfig.lib.json",
        "project": "libs/feature/react/game/package.json",
        "entryFile": "libs/feature/react/game/src/index.ts",
        "external": ["react", "react-dom", "react/jsx-runtime"],
        "rollupConfig": "@nx/react/plugins/bundle-rollup",
        "compiler": "babel",
        "assets": [
          {
            "glob": "libs/feature/react/game/README.md",
            "input": ".",
            "output": "."
          }
        ]
      }
    }
  }
}
